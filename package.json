{
  "name": "next-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && npx concurrently 'npm run cld-dev' 'next dev'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "push": "npx prisma db push",
    "pgen": "npx prisma generate --no-engine",
    "cld-deploy": "wrangler deploy",
    "cld-dev": "wrangler dev --port=50000",
    "ppush": "dotenv -e .env.local -- yarn prisma db push --skip-generate"
  },
  "dependencies": {
    "@next/font": "13.1.4",
    "@prisma/client": "5.4.2",
    "bridg": "1.1.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.4",
    "next": "13.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "typescript": "4.9.4",
    "wrangler": "^3.0.0",
    "@types/node": "18.11.18",
    "@types/react-dom": "18.0.10",
    "@types/react": "18.0.27",
    "@cloudflare/workers-types": "^4.20230419.0",
    "prisma": "5.4.2"
  }
}
